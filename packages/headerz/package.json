{
  "name": "headerz",
  "version": "0.0.0",
  "description": "Type helpers for headers",
  "author": "Tim Morris <timthewebguy@gmail.com>",
  "homepage": "https://github.com/tkofh/headerz#readme",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./cache-control": {
      "require": "./dist/cacheControl/index.cjs",
      "import": "./dist/cacheControl/index.mjs",
      "types": "./dist/cacheControl/index.d.ts"
    }
  },
  "files": ["dist"],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkofh/headerz.git"
  },
  "scripts": {
    "build": "unbuild",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublish": "bun run build"
  },
  "bugs": {
    "url": "https://github.com/tkofh/headerz/issues"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "unbuild": "^2.0.0",
    "vitest": "^2.0.5"
  },
  "dependencies": {}
}
